{
    "version": 2,
    "env": {
        "CLIENT_URL": "https://medical-test-tracking.vercel.app"
    },
    "builds": [
        {
            "src": "backend/dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/dist/main.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
        }
        
    ]
}