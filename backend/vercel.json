{
    "version": 2,
    "env": {
      "CLIENT_URL": "https://medical-test-tracking.vercel.app"
    },
    "builds": [
      {
        "src": "dist/main",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main"
      }
    ]
  }
  